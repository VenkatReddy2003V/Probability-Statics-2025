      "source": [
        "import matplotlib.pyplot as plt\n",
        "import seaborn as sns\n",
        "import numpy as np\n",
        "\n",
        "unique = np.array([1, 2, 3, 4, 5])\n",
        "cdf = np.array([0.1, 0.3, 0.6, 0.8, 1.0])\n",
        "\n",
        "plt.figure(figsize=(10, 5))\n",
        "sns.lineplot(x=unique, y=cdf, marker='o', color='red', linestyle='-')\n",
        "plt.xlabel(\"Number of Children Per Family\")\n",
        "plt.ylabel(\"Cumulative Probability (CDF)\")\n",
        "plt.title(\"Cumulative Distribution Function (CDF) of Family Sizes\")\n",
        "plt.xticks(unique)\n",
        "plt.ylim(0, 1)\n",
        "plt.show()\n",
        "\n"
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "import numpy as np\n",
        "import matplotlib.pyplot as plt\n",
        "from collections import Counter\n",
        "\n",
        "np.random.seed(42)\n",
        "N = 1000\n",
        "vehicle_data = np.random.randint(0, 6, size=N)\n",
        "\n",
        "vehicle_counts = Counter(vehicle_data)\n",
        "pmf = {k: v / N for k, v in vehicle_counts.items()}\n",
        "\n",
        "cdf = {}\n",
        "cumulative_prob = 0\n",
        "for k in sorted(pmf.keys()):\n",
        "    cumulative_prob += pmf[k]\n",
        "    cdf[k] = cumulative_prob\n",
        "\n",
        "fig, ax = plt.subplots(1, 2, figsize=(14, 6))\n",
        "\n",
        "ax[0].bar(pmf.keys(), pmf.values(), color='blue', alpha=0.6, label=\"PMF\")\n",
        "ax[0].set_title(\"Probability Mass Function (PMF)\")\n",
        "ax[0].set_xlabel(\"Number of Vehicles\")\n",
        "ax[0].set_ylabel(\"Probability\")\n",
        "ax[0].set_xticks(np.arange(0, 6))\n",
        "ax[0].legend()\n",
        "\n",
        "ax[1].step(sorted(cdf.keys()), [cdf[k] for k in sorted(cdf.keys())], color='red', label=\"CDF\", where=\"post\")\n",
        "ax[1].set_title(\"Cumulative Distribution Function (CDF)\")\n",
        "ax[1].set_xlabel(\"Number of Vehicles\")\n",
        "ax[1].set_ylabel(\"Cumulative Probability\")\n",
        "ax[1].legend()\n",
        "\n",
        "plt.tight_layout()\n",
        "plt.show()\n",
        "\n"
      ],
      "metadata": {
