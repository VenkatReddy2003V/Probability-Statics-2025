      "cell_type": "code",
      "source": [
        "import scipy.stats as stats\n",
        "\n",
        "N = 100\n",
        "p = 0.05\n",
        "\n",
        "prob_10_defective = stats.binom.pmf(10, N, p)\n",
        "prob_at_least_15_defective = 1 - stats.binom.cdf(14, N, p)\n",
        "expected_defective = N * p\n",
        "\n",
        "print(\"Probability that exactly 10 gadgets are defective:\", prob_10_defective)\n",
        "print(\"Probability that at least 15 gadgets are defective:\", prob_at_least_15_defective)\n",
        "print(\"Expected number of defective gadgets:\", expected_defective)\n"
      ],
      "metadata": {
